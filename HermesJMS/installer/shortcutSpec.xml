<?xml version="1.0" encoding="iso-8859-1" standalone="yes" ?>

 <shortcuts>
       <skipIfNotSupported/>
       <programGroup defaultName="HermesJMS"
		     location="startMenu"/>
          <shortcut  name="Hermes"
	             target="$INSTALL_PATH\bin\hermes.bat"
		     iconFile="$INSTALL_PATH\bin\hermes-32x32.ico"
		     commandLine=""
		     workingDirectory="$INSTALL_PATH\bin"
		     description="Start HermesJMS Browser"
		     initialState="noShow"
		     programGroup="yes"
		     desktop="no"
		     applications="no"
		     startMenu="no"
		     startup="no">
		     
		     <createForPack name="HermesJMS"/>
          </shortcut>

          <shortcut name="Uninstall Hermes"
	             target="$INSTALL_PATH\Uninstaller\uninstaller.jar"
   		     iconFile="$INSTALL_PATH\bin\recycle.ico"
		     commandLine=""
		     workingDirectory="."
		     description="Uninstall HermesJMS Browser"
		     initialState="normal"
		     programGroup="yes"
		     desktop="no"
		     applications="no"
		     startMenu="no"
		     startup="no">
		     
		     <createForPack name="HermesJMS"/>
           </shortcut>
</shortcuts>